{% import (Demo) from "../../../widgets/demo" %}
{% import (MultiSelect) from "../../../../../../lib/control/multi-select" %}
{% import (MultiSelectPage) from "../" %}

{% view Main (MultiSelectPage) %}
 
<Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.normal.id}}
     ww:name={{@values.normal.name}}
     ww:label={{@values.normal.label}}
     ww:stringifier={{@values.normal.stringifier}}
     ww:onSearch={{@values.normal.onSearch}}
     ww:onChange={{@values.normal.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.success.id}}
     ww:name={{@values.success.name}}
     ww:label={{@values.success.label}}
     ww:success={{@values.success.message}}
     ww:stringifier={{@values.success.stringifier}}
     ww:onSearch={{@values.success.onSearch}}
     ww:onChange={{@values.success.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.warning.id}}
     ww:name={{@values.warning.name}}
     ww:label={{@values.warning.label}}
     ww:warning={{@values.warning.message}}
     ww:stringifier={{@values.warning.stringifier}}
     ww:onSearch={{@values.warning.onSearch}}
     ww:onChange={{@values.warning.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.error.id}}
     ww:name={{@values.error.name}}
     ww:label={{@values.error.label}}
     ww:error={{@values.error.message}}
     ww:stringifier={{@values.error.stringifier}}
     ww:onSearch={{@values.error.onSearch}}
     ww:onChange={{@values.error.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.block.id}}
     ww:name={{@values.block.name}}
     ww:label={{@values.block.label}}
     ww:block=true
     ww:stringifier={{@values.block.stringifier}}
     ww:onSearch={{@values.block.onSearch}}
     ww:onChange={{@values.block.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     ww:name={{@values.normal.name}}
     ww:label="Disabled"
     ww:disabled=true
     ww:onSearch={{@values.normal.onSearch}}
     ww:onChange={{@values.normal.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     ww:name={{@values.init.name}}
     ww:label="Initialized"
     ww:value={{@values.init.value}}
     ww:stringifier={{@values.init.stringifier}}
     ww:onSearch={{@values.init.onSearch}}
     ww:onChange={{@values.init.onChange}} />

  </Demo>

</Demo>
