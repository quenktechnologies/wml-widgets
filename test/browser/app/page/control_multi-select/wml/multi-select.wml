{% import (Demo) from "../../../widgets/demo" %}
{% import (MultiSelect) from "../../../../../../lib/control/multi-select" %}
{% import (MultiSelectPage) from "../" %}

{% view Main (MultiSelectPage) %}
 
<Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.normal.id}}
     name={{@values.normal.name}}
     label={{@values.normal.label}}
     stringifier={{@values.normal.stringifier}}
     onSearch={{@values.normal.onSearch}}
     onChange={{@values.normal.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.success.id}}
     name={{@values.success.name}}
     label={{@values.success.label}}
     success={{@values.success.message}}
     stringifier={{@values.success.stringifier}}
     onSearch={{@values.success.onSearch}}
     onChange={{@values.success.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.warning.id}}
     name={{@values.warning.name}}
     label={{@values.warning.label}}
     warning={{@values.warning.message}}
     stringifier={{@values.warning.stringifier}}
     onSearch={{@values.warning.onSearch}}
     onChange={{@values.warning.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.error.id}}
     name={{@values.error.name}}
     label={{@values.error.label}}
     error={{@values.error.message}}
     stringifier={{@values.error.stringifier}}
     onSearch={{@values.error.onSearch}}
     onChange={{@values.error.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     wml:id={{@values.block.id}}
     name={{@values.block.name}}
     label={{@values.block.label}}
     block=true
     stringifier={{@values.block.stringifier}}
     onSearch={{@values.block.onSearch}}
     onChange={{@values.block.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     name={{@values.normal.name}}
     label="Disabled"
     disabled=true
     onSearch={{@values.normal.onSearch}}
     onChange={{@values.normal.onChange}} />

  </Demo>

  <Demo>

    <MultiSelect 
     name={{@values.init.name}}
     label="Initialized"
     value={{@values.init.value}}
     stringifier={{@values.init.stringifier}}
     onSearch={{@values.init.onSearch}}
     onChange={{@values.init.onChange}} />

  </Demo>

</Demo>
