import * as Class from "wat-classes";

<header class={{Class.LAYOUT_LOGOIMAGE}}>

  {% if this.attributes.read('wat:image', false) %}

      <h1>

        <a href={{this.attributes.read('wat:href', '#')}}>

          <img
            class={{Class.LAYOUT_LOGOIMAGE_IMAGE}}
            src={{this.attributes.read('wat:image')}}
            alt={{this.attributes.read('wat:alt')}}/>

        </a>

      </h1>

  {% endif %}

</header>
