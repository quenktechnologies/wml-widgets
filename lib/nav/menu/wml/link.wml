import {Link} from "@package/self/nav/link"
import {Link as LinkContext} from "../Link"
import {Item} from "../Item"

{% main (LinkContext) %}

<Item
  ww:class={{@values.item.class}}>

  <Link
    ww:class={{@values.a.class}}
    ww:name={{@values.a.name}}
    ww:title={{@values.a.title}}
    ww:href={{@values.a.href}}
    ww:text={{@values.a.text}}
    ww:active={{@values.a.active}}
    ww:onClick={{@values.a.onClick}}>{{@children}}</Linksz>

</Item>
