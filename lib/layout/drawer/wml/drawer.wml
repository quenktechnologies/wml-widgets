import {Aside} from "../../aside/Aside"
import {Drawer} from "../Drawer"

{% main (Drawer) %}

<div 
  wml:id={{@values.id.root}}
  class={{@values.class.root}}>

  <Aside
    wml:id={{@values.id.drawer}}
    ww:content={{@values.aside.content}}/>

  {% if @content %}

    {{@content}}
    
  {% else if @values.content %}

    {{ @values.content.render() }}

  {% else %}

    {{@children}}

  {% endif %}

</div>
