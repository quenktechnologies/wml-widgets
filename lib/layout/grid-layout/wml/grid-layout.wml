import * as G from ".."

{% view Grid (G.GridLayout) %}

  <section 
    wml:id={{@values.content.id}}
    class={{@values.content.class}}>
    {{@children}}
  </section>

{% endview %}

{% view Row (G.Row) %}

  <div 
    wml:id={{@values.content.id}}
    class={{@values.content.class}}>
    {{@children}}
  </div>

{% endview %}

{% view Column (G.Column) %}

  <div 
    wml:id={{@values.content.id}}
    class={{@values.content.class}}>
    {{@children}}
  </div>

{% endview %}
