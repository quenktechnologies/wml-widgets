import {Checkbox} from "../Checkbox"

{% main (Checkbox) %}

<div class={{@values.class.root}}>

  <label>

    <input 
      type="checkbox"
      name={{@values.input.name}}
      checked={{@values.input.checked || null}}
      onchange={{@values.input.onChange}}/> {{@children}}

  </label>

</div>

