{% import (Input) from "../" %}

{% view Main (Input) %}
<input
  id={{@values.id}}
  name={{@values.name}}
  type={{@values.type}}
  placeholder={{@values.placeholder}}
  oninput={{@values.oninput}} 
  value={{@values.value}}
  disabled={{@values.disabled}}
  readonly={{@values.readOnly}}
  class={{@values.className}} />
