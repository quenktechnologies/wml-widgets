import {Button} from '../Button';

{% main (Button) %}
  
<button
  wml:id={{@values.button.id}}
  id={{@values.button.id}}
  type={{@values.button.type}}
  name={{@values.button.name}}
  disabled={{@values.button.disabled}}
  class={{@values.button.class}}
  onclick={{@values.button.onclick}}>
  {{@values.button.text}}
  {{@children}}
</button>
