{
  "name": "@quenk/wml-widgets",
  "version": "0.1.0",
  "description": "Various wml widgets for creating web apps.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --opts mocha.opts test",
    "build": "rm -R lib/*; mkdir -p lib; ./node_modules/.bin/babel -D src -s inline -d lib",
    "example": "NODE_PATH=src:src/lib ./node_modules/.bin/browserify --debug -t ./node_modules/wmljs/lib/transform -t babelify examples/src/Application.js > examples/js/app.js;./node_modules/.bin/lessc --include-path=less less/theme/default/theme.less > examples/css/theme.css"
  },
  "dependencies": {
    "@quenk/wml": "^0.1.3",
    "beof": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "nop": "^1.0.0"
  },
  "devDependencies": {
    "@quenk/wml-cli": "0.0.1",
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "less": "^2.7.1",
    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "property-seek": "^0.1.0",
    "wat-classes": "file:lib/lib/wat-classes"
  },
  "author": "Lasana Murray <dev@quenk.com> (http://quenk.com)",
  "license": "Apache-2.0"
}
