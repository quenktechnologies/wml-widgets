import {Grid,Row,Column} from "@package/wml-widgets/layout/grid/Grid"
import {Table} from "@package/wml-widgets/table/Table"
import {TablePage} from "../"

{% main (TablePage) %}
 
<Grid>

  <Row>

    <Column>

      <Table
        ww:selectable=true
        ww:data={{@values.users}}
        ww:columns={{@values.columns}}
        ww:onCellClicked={{@values.onCellClicked}}/>

    </Column>

   </Row>

 </Grid>
