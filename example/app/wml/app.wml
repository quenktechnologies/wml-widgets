import {Drawer} from "@package/wml-widgets/layout/drawer/Drawer"
import {ActionBar} from "@package/wml-widgets/app/action-bar/ActionBar"
import {App} from "../app"
import {IconButton} from "@package/wml-widgets/control/icon-button/IconButton"
import {Dash} from "@package/wml-widgets/app/dash"
import {Main as MainLayout} from "@package/wml-widgets/layout/main/Main"

{% main (App) %}

<Drawer
  wml:id={{@values.id.layout}}
  ww:drawer={{@navigation}}>

  <ActionBar>

    <IconButton ww:onClick={{@toggleDrawer}}>

      <Dash/>
      <Dash/>
      <Dash/>

    </IconButton>

  </ActionBar>

  <MainLayout>
    {{@content.render()}}
  </MainLayout>

</Drawer>
