import {Grid,Row,Column} from "../../../../../lib/layout/grid/Grid"
import {Select} from "../../../../../lib/control/select/Select"
import {SelectPage} from "../"

{% main (SelectPage) %}
 
<Grid>

  <Row>

    <Column>

      <p>You selected: <b wml:id="selected">(None)</b>.</p>

      <p>
        <Select 
          wml:id="select"
          ww:name="select"
          ww:options={{@values.options}}
          ww:onChange={{@onChange}} />
      </p>

    </Column>

   </Row>

 </Grid>
