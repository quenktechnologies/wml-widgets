import {Grid,Row,Column} from "@package/wml-widgets/layout/grid/Grid"
import {Button} from "@package/wml-widgets/control/button"
import {ButtonPage} from "../"

{% main (ButtonPage) %}
 
<Grid>

  <Row>

    <Column>

      <p>

        {% for v:String,k:String in @values.styles %}

          <Button 
            ww:name={{v}}
            ww:style={{v}}
            ww:text={{k|@values.capitalize}} />

        {% endfor %}

      </p>

    </Column>

   </Row>

 </Grid>
