import {Aside} from "../Aside"

{% main (Aside) %}

<div
  wml:id={{@values.id.root}}
  class={{@values.class.root}}>

  <div class={{@values.class.content}}>

      {% if @values.content %}

        {{ @values.content.render() }}

      {% else %}

        {{@children}}

      {% endif %}

  </div>

</div>
