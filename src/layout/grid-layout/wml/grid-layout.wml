import * as G from ".."

{% view Grid (G.GridLayout) %}

  <section class={{@values.class.root}}>
    {{@children}}
  </section>

{% endview %}

{% view Row (G.Row) %}

  <div class={{@values.class.root}}>
    {{@children}}
  </div>

{% endview %}

{% view Column (G.Column) %}

  <div class={{@values.class.root}}>
    {{@children}}
  </div>

{% endview %}
