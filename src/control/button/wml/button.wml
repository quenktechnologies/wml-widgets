import * as B from '../Button';

{% view Group (B.Group) %}

  <div 
   class={{@getClass}}>{{@children}}</div>

{% endview %}

{% view Button (B.Button) %}
  
      <button
        wml:id="button"
        type={{@values.button.type}}
        name={{@values.button.name}}
        disabled={{@values.button.disabled}}
        class={{@values.button.class}}
        onclick={{@values.button.onclick}}>
  
        {{@values.button.text}}
        {{@children}}
  
      </button>
  
{% endview %}
