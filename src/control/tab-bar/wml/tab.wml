import {Tab} from ".."

{% main (Tab) %}
<li 
  wml:id={{@values.root.id}}
  class={{@values.root.class}}>

    <a 
      wml:id={{@values.a.id}}
      href="#"
      onclick={{@values.a.clicked}}>

        {% if @values.a.text %}

          {{@values.a.text}}

        {% else %}

          {{@children}}

        {% endif %}

    </a> 

</li>
