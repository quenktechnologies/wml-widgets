import {Tab} from ".."

{% main (Tab) %}
<li 
  wml:id={{@values.id.root}}
  class={{@values.class.li}}>

    <a 
      wml:id={{@values.id.a}}
      href="#"
      onclick={{@clicked}}>

        {% if @values.tab.text %}

          {{@values.tab.text}}

        {% else %}

          {{@children}}

        {% endif %}

    </a> 

</li>
