{% import (Search) from "../" %}

{% view Main (Search) %}
<input 
  wml:id={{@values.root.wml.id}}
  id={{@values.root.id}}
  class={{@values.root.className}}
  onkeydown={{@values.root.onkeydown}} 
  onkeyup={{@values.root.onkeyup}} 
  oninput={{@values.root.oninput}}
  onfocus={{@values.root.onfocus}}
  placeholder={{@values.root.placeholder}}
  readOnly={{@values.root.readOnly}}
  value={{@values.root.value}} />
