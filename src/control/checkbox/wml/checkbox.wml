{% import (Checkbox) from "../" %}

{% view Main (Checkbox) %}

<div id={{@values.root.id}} class={{@values.root.className}}>

  <label>

    <input 
      type="checkbox"
      name={{@values.input.name}}
      checked={{@values.input.value}}
      onchange={{@values.input.onChange}}/> {{...@children}}

  </label>

</div>

