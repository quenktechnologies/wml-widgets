{% import * as p from "../" %}

{% view Thumbnail (p.Thumbnail) %}
<div
  id={{@values.id}} 
  class={{@values.className}}
  onclick={{@values.onclick}}>

  {{...@children}}

</div>

{% view Anchor (p.Thumbnail) %}
<a
  id={{@values.id}} 
  class={{@values.className}}
  href={{@values.href}}
  onclick={{@values.onclick}}>

  {{...@children}}

</div>


{% view Caption (p.Caption) %}
<div
  id={{@values.id}}
  class={{@values.className}}>

  {{...@children}}

</div>
