import * as Styles from "wml-widgets-common/Styles"
import {Drawer} from "../../drawer/Drawer"

<div 
  wml:id="content"
  class={{Styles.DRAWER_LAYOUT}}>

  <Drawer 
    wml:id="drawer"
    ww:content={{@.read("ww:drawer")}}/>

  {% if @.read("ww:content") %}

    {% call (@.read("ww.content")) %}

  {% else %}

    {{this.children}}

  {% endif %}

</div>
