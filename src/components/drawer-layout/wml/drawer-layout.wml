import * as Styles from 'common/Styles';

<div class={{Styles.DRAWER_LAYOUT}}>

  <div wml:id="drawer" class={{Styles.DRAWER}}>

    <div class={{Styles.DRAWER_CONTENT}}>

      {% if this.attributes.read('ww:navigation') %}

        {% include this.attributes.read('ww:navigation') %}

      {% endif %}

    </div>

  </div>

  <div class={{this._combine([Styles.MAIN_VIEW, Styles.DRAWER_PUSHABLE])}}>

    {{this.children}}

  </div>

</div>
