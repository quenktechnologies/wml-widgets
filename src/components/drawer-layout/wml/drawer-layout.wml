import * as Styles from 'wml-widgets-common/Styles'

<div wml:id="content" class={{Styles.DRAWER_LAYOUT}}>

  <div wml:id="drawer" class={{Styles.DRAWER}}>

    <div class={{Styles.DRAWER_CONTENT}}>

      {% if @.read('ww:navigation') %}

        {% call (@.read('ww:navigation')) %}

      {% endif %}

    </div>

  </div>

  {% if @.read('ww:content') %}

    {% call (@.read('ww.content')) %}

  {% else %}

    {{this.children}}

  {% endif %}

</div>
